            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "latsnp",
            "title": "Close Long Position [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "snpnp",
            "title": "Open Short Positon [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "sabbnp",
            "title": "Close Short Position [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "enLong",
            "title": "Enable Long",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "true"
          },
          {
            "name": "enShort",
            "title": "Enable Short",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "false"
