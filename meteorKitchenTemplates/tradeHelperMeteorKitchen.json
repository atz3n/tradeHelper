{
  "application": {
    "title": "Trade Helper",
    "theme": "bootswatch-simplex",
    "on_user_created_code": "Settings.insert({ enNotification: 'true', dummy: 'false', ownerId: user._id, modifiedBy: user._id });",
    "roles": [
      "admin",
      "user"
    ],
    /***********************************************************************
  Global Config
 ***********************************************************************/
    "default_role": "user",
    "send_verification_email": false,
    /***********************************************************************
  Collections
 ***********************************************************************/
    "collections": [
      {
        "name": "strategies",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "updateTime",
            "title": "Update Time [min]",
            "type": "integer",
            "min": 1,
            "default": 10,
            "required": true
          },
          {
            "name": "orPlugins",
            "title": "Plugin Bundles (logical connected via 'or')",
            "type": "array",
            "array_item_type": "object",
            "input": "crud",
            "crud_fields": [
              {
                "name": "andPlugins",
                "title": "Plugin Bundle",
                "type": "string",
                "input": "select",
                "lookup_query_name": "pluginbundles",
                "lookup_field": "name",
                "lookup_key": "_id",
                "required": true
              }
            ],
            "required": true
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "pluginbundles",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string"
          },
          {
            "name": "bundlePlugins",
            "title": "Plugins (logical connected via 'and')",
            "type": "array",
            "array_item_type": "object",
            "input": "crud",
            "crud_fields": [
              {
                "name": "bundlePlugin",
                "title": "Plugin",
                "type": "string",
                "input": "select",
                "lookup_query_name": "swings",
                "lookup_field": "name",
                "lookup_key": "_id",
                "required": true
              }
            ],
            "required": true
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
