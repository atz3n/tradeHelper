            "name": "platform",
            "title": "Platform",
            "type": "string",
            "input": "select",
            "lookup_query_name": "krakens",
            "lookup_field": "name",
            "lookup_key": "_id",
            "required": true
          },
          {
            "name": "lnpnp",
            "title": "Open Long Position [%]",
