{
  "application": {
    "title": "Trade Helper",
    "theme": "bootswatch-simplex",
    "on_user_created_code": "Settings.insert({ enNotification: 'true', dummy: 'false', ownerId: user._id, modifiedBy: user._id });",
    "roles": [
      "admin",
      "user"
    ],
    /***********************************************************************
  Global Config
 ***********************************************************************/
    "default_role": "user",
    "send_verification_email": false,
    /***********************************************************************
  Collections
 ***********************************************************************/
    "collections": [
      {
        "name": "settings",
        "fields": [
          {
            "name": "enNotification",
            "title": "Enable Notifications",
            "type": "bool",
            "input": "checkbox",
            "default": "true"
          },
          {
            "name": "dummy",
            "title": "Dummy",
            "type": "bool",
            "input": "checkbox",
            "default": "false"
          }

        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "swings",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "type",
            "title": "Type (Do not change, it's a workaround)",
            "type": "string",
            "default": "swingType",
            "show_in_insert_form": true,
            "show_in_update_form": false,
            "show_in_dataview": false,
            "required": true
          },
          {
            "name": "lnpnp",
            "title": "Open Long Position [%]",
            "type": "float",
            "default": "5.0",
            "min": "0",
            "required": true
          },
          {
            "name": "latsnp",
            "title": "Close Long Position [%]",
            "type": "float",
            "default": "5.0",
            "min": "0",
            "required": true
          },
          {
            "name": "snpnp",
            "title": "Open Short Positon [%]",
            "type": "float",
            "default": "5.0",
            "min": "0",
            "required": true
          },
          {
            "name": "sabbnp",
            "title": "Close Short Position [%]",
            "type": "float",
            "default": "5.0",
            "min": "0",
            "required": true
          },
          {
            "name": "enLong",
            "title": "Enable Long",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "true"
          },
          {
            "name": "enShort",
            "title": "Enable Short",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "false"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "dummys",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "type",
            "title": "Type (Do not change, it's a workaround)",
            "type": "string",
            "default": "dummyType",
            "show_in_insert_form": true,
            "show_in_update_form": false,
            "show_in_dataview": false,
            "required": true
          },
          {
            "name": "dummyCont",
            "title": "Dummy Content",
            "type": "string"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      }
    ],
    /***********************************************************************
  Queries
 ***********************************************************************/
    "queries": [
      {
        "name": "dummys",
        "collection": "dummys",
        "filter": {}
      },
      {
        "name": "dummys_empty",
        "collection": "dummys",
        "filter": {
          "_id": null
        },
        "find_one": true
      },
      {
        "name": "dummy",
        "collection": "dummys",
        "filter": {
          "_id": ":dummyId"
        },
        "find_one": true
      },
      {
        "name": "swings",
        "collection": "swings",
        "filter": {}
      },
      {
        "name": "swings_empty",
        "collection": "swings",
        "filter": {
          "_id": null
        },
        "find_one": true
      },
      {
        "name": "swing",
        "collection": "swings",
        "filter": {
          "_id": ":swingId"
        },
        "find_one": true
      },
      {
        "name": "settings",
        "collection": "settings",
        "filter": {}
      },
      {
        "name": "settings_empty",
        "collection": "settings",
        "filter": {
          "_id": null
        },
        "find_one": true
      },
      {
        "name": "settings_first",
        "collection": "settings",
        "filter": {},
        "find_one": true
      },
      {
        "name": "setting",
        "collection": "settings",
        "filter": {
          "_id": ":settingId"
        },
        "find_one": true
      },
      {
        "name": "admin_users",
        "collection": "users"
      },
      {
        "name": "admin_user",
        "collection": "users",
        "filter": {
          "_id": ":userId"
        },
        "find_one": true
      },
      {
        "name": "users_null",
        "collection": "users",
        "filter": {
          "_id": null
        },
        "find_one": true
      },
      {
        "name": "current_user_data",
        "collection": "users",
        "filter": {
          "_id": "Meteor.userId()"
        },
        "find_one": true
      }
    ],
    /***********************************************************************
  Public
 ***********************************************************************/
    "public_zone": {
      "pages": [
        {
          "name": "about",
          "title": "In process...",
          "type": "page"
