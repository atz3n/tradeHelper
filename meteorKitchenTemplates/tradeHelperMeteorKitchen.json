        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "dummys",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "type",
