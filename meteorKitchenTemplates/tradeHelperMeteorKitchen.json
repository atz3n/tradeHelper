            "name": "pair",
            "title": "Currency Pair",
            "type": "string",
            "input": "radio",
            "input_items": [
              {
                "title": "Ether/Euro",
                "value": "eth_eur"
              },
              {
                "title": "Ether/Bitcoin",
                "value": "eth_btc"
              },
              {
                "title": "Bitcoin/Euro",
                "value": "btc_eur"
