{
  "application": {
    "title": "Trade Helper",
    "theme": "bootswatch-simplex",
    "on_user_created_code": "Settings.insert({ enNotification: 'true', dummy: 'false', ownerId: user._id, modifiedBy: user._id });",
    "roles": [
      "admin",
      "user"
    ],
    /***********************************************************************
  Global Config
 ***********************************************************************/
    "default_role": "user",
    "send_verification_email": false,
    /***********************************************************************
  Collections
 ***********************************************************************/
    "collections": [
      {
        "name": "strategies",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "updateTime",
            "title": "Update Time [min]",
            "type": "integer",
            "min": 1,
            "default": 10,
            "required": true
          },
          {
            "name": "mode",
            "title": "Mode",
            "type": "string",
            "input": "radio",
            "input_items": [
              {
                "title": "Automatic",
                "value": "auto"
              },
              {
                "title": "Manual",
                "value": "manual"
              }
            ],
            "required": true
          },
          {
            "name": "pluginBundles",
            "title": "Plugin Bundles (logical connected via 'or')",
            "type": "array",
            "array_item_type": "object",
            "input": "crud",
            "crud_fields": [
              {
                "name": "bundle",
                "title": "Plugin Bundle",
                "type": "string",
                "input": "select",
                "lookup_query_name": "pluginbundles",
                "lookup_field": "name",
                "lookup_key": "_id",
                "required": true
              }
            ],
            "required": true
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "activeDatas",
        "fields": [
          {
            "name": "data",
            "type": "string"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "nobody"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "pluginBundles",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "bundlePlugins",
            "title": "Plugins (logical connected via 'and')",
            "type": "array",
            "array_item_type": "object",
            "input": "crud",
            "crud_fields": [
              {
                "name": "plugin",
                "title": "Plugin",
                "type": "string",
                "input": "select",
                "lookup_query_name": "plSwings",
                "lookup_field": "name",
                "lookup_key": "_id",
                "required": true
                
              }
            ],
            "required": true
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      },
      {
        "name": "plSwings",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "exchange",
            "title": "Exchange",
            "type": "string",
            "input": "select",
            "lookup_query_name": "exKrakens",
            "lookup_field": "name",
            "lookup_key": "_id",
            "required": true,
            "show_in_dataview": false,
            "show_in_read_only_form": false,
            "exportable": false,
            "join_collection": "exKrakens",
            "join_fields": [
              "name"
            ],
            "join_container": "exKrakenCont"
          },
          {
            "name": "exKrakenCont.name",
            "title": "Exchange",
            "show_in_insert_form": false,
            "show_in_update_form": false
          },
          {
            "name": "lnpnp",
            "title": "Open Long Position [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "latsnp",
            "title": "Close Long Position [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "snpnp",
            "title": "Open Short Positon [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "sabbnp",
            "title": "Close Short Position [%]",
            "type": "float",
            "default": 5,
            "min": 0,
            "required": true
          },
          {
            "name": "enLong",
            "title": "Enable Long",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "true"
          },
          {
            "name": "enShort",
            "title": "Enable Short",
            "type": "bool",
            "input": "checkbox",
            "display_helper": "booleanToYesNo",
            "default": "false"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ],
        "before_insert_code": "doc.type = 'plSwing';"
        
      },
      {
        "name": "plDummys",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "dummyCont",
            "title": "Dummy Content",
            "type": "string"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ],
        "before_insert_code": "doc.type = 'plDummy';"
      },
      {
        "name": "exKrakens",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "pair",
            "title": "Currency Pair",
            "type": "string",
            "input": "radio",
            "input_items": [
              {
                "title": "Ether/Euro",
                "value": "Ether/Euro"
              },
              {
                "title": "Ether/Bitcoin",
                "value": "Ether/Bitcoin"
              },
              {
                "title": "Bitcoin/Euro",
                "value": "Bitcoin/Euro"
              }
            ],
            "required": true
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ],
        "before_insert_code": "doc.type = 'exKraken';"
      },
      {
        "name": "exTestDatas",
        "fields": [
          {
            "name": "name",
            "title": "Name",
            "type": "string",
            "required": true
          },
          {
            "name": "gain",
            "title": "Gain (multiplier)",
            "type": "float",
            "default": 1,
            "required": true
          },
          {
            "name": "counter",
            "title": "Counter (starts with this value)",
            "type": "float",
            "default": 0,
            "required": true
          },
          {
            "name": "priceType",
            "title": "Price Type",
            "type": "string",
            "input": "radio",
            "input_items": [
              {
                "title": "Sinus",
                "value": "Sinus"
              },
              {
                "title": "Data",
                "value": "Data"
              }
            ],
            "required": true
          },
          {
            "name": "data",
            "title": "Data",
            "type": "float",
            "input": "textarea"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ],
        "before_insert_code": "doc.type = 'exTestData';"
      },
      {
        "name": "settings",
        "fields": [
          {
            "name": "enNotification",
            "title": "Enable Notifications",
            "type": "bool",
            "input": "checkbox",
            "default": "true"
          },
          {
            "name": "dummy",
            "title": "Dummy",
            "type": "bool",
            "input": "checkbox",
            "default": "false"
          }
        ],
        "owner_field": "ownerId",
        "roles_allowed_to_read": [
          "owner"
        ],
        "roles_allowed_to_update": [
          "owner"
        ],
        "roles_allowed_to_delete": [
          "owner"
        ]
      }
